{
  "name": "newsletter_backend",
  "version": "1.0.0",
  "description": "O backend de um Newsletter inteligente",
  "homepage": "https://github.com/Maiamon/newsletter_backend#readme",
  "bugs": {
    "url": "https://github.com/Maiamon/newsletter_backend/issues"
  },
  "imports": {
    "#src/*": "./src/*",
    "#lib/*": "./src/lib/*",
    "#http/*": "./src/http/*",
    "#repositories/*": "./src/repositories/*",
    "#use-cases/*": "./src/use-cases/*"
  },
  "prisma": {
    "seed": "node prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maiamon/newsletter_backend.git"
  },
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "build": "tsup src/server.ts --out-dir build",
    "dev": "tsx --watch src/server.ts",
    "start": "node build/server.js",
    "lint": "eslint . --ext .ts,.js,.mts,.cts",
    "lint:fix": "eslint . --ext .ts,.js,.mts,.cts --fix"
  },
  "dependencies": {
    "@prisma/client": "6.16.2",
    "bcryptjs": "3.0.2",
    "dotenv": "17.2.2",
    "fastify": "5.6.0",
    "jose": "6.1.0",
    "jsonwebtoken": "9.0.2",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@eslint/json": "0.13.2",
    "@eslint/markdown": "7.2.0",
    "@types/node": "24.5.2",
    "eslint": "9.36.0",
    "globals": "16.4.0",
    "jiti": "2.5.1",
    "prisma": "6.16.2",
    "tsup": "8.5.0",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.0",
    "vitest": "3.2.4"
  }
}
